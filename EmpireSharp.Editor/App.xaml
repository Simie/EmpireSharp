<Application x:Class="EmpireSharp.Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:studio="clr-namespace:Papyrus.Studio;assembly=Papyrus.Studio">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>

                    <studio:EditorBootstrapper x:Key="bootstrap">
                        <studio:EditorBootstrapper.Modules>
                            <sys:String>EmpireSharp.Data</sys:String>
                        </studio:EditorBootstrapper.Modules>
                    </studio:EditorBootstrapper>

                </ResourceDictionary>
                <ResourceDictionary Source="/Papyrus.Studio;component/Framework/Themes/Generic.xaml"/>
                <ResourceDictionary Source="/Framework/Themes/Generic.xaml"/>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
